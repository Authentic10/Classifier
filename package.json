{
  "name": "svelte-app",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "css-build": "node-sass --omit-source-map-url src/styles/main.scss public/global.css",
    "css-watch": "npm run css-build -- --watch",
    "build": "rollup -c",
    "dev": "rollup -c -w",
    "start": "sirv public -s",
    "validate": "svelte-check"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^17.0.0",
    "@rollup/plugin-node-resolve": "^11.0.0",
    "@rollup/plugin-typescript": "^8.0.0",
    "@tsconfig/svelte": "^1.0.0",
    "autoprefixer": "^10.2.5",
    "node-sass": "^5.0.0",
    "raw-loader": "^4.0.0",
    "rollup": "^2.3.4",
    "rollup-plugin-css-only": "^3.1.0",
    "rollup-plugin-livereload": "^2.0.0",
    "rollup-plugin-svelte": "^7.0.0",
    "rollup-plugin-terser": "^7.0.0",
    "svelte": "^3.0.0",
    "svelte-check": "^1.0.0",
    "svelte-preprocess": "^4.6.9",
    "tslib": "^2.0.0",
    "typescript": "^4.0.0"
  },
  "dependencies": {
    "bulma": "^0.9.2",
    "postcss-scss": "^3.0.5",
    "sirv-cli": "^1.0.0",
    "svelte-routing": "^1.5.0"
  }
}
